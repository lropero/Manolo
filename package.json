    "pokersolver": "^2.1.4",
    "rxjs": "^6.6.3",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2"
  },
  "author": "Luciano Ropero <lropero@gmail.com>",
  "husky": {
    "hooks": {
