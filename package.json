{
  "name": "manolo",
  "version": "0.1.0",
  "private": true,
  "description": "A poker (Texas hold'em) data generator for testing Reinforcement Learning algorithms.",
  "license": "UNLICENSED",
  "author": "Luciano Ropero <lropero@gmail.com>",
  "type": "module",
  "scripts": {
    "audit": "npx upgradeps -v",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json",
    "lint": "npx prettier --write . && npx standard --fix",
    "nuke": "npm run clean && npm cache verify && npm install",
    "sort": "npx sort-package-json",
    "start": "node --max-old-space-size=4096 index.js"
  },
  "dependencies": {
    "array-shuffle": "3.0.0",
    "chalk": "5.2.0",
    "figures": "5.0.0",
    "gradient-string": "2.0.2",
    "lodash": "4.17.21",
    "pokersolver": "2.1.4",
    "rxjs": "7.8.0",
    "strip-ansi": "7.0.1"
  },
  "devDependencies": {
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "standard": "17.0.0",
    "upgradeps": "2.0.5"
  }
}
